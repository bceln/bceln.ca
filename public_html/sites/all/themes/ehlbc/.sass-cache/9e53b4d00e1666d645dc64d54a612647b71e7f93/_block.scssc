@import "compass/css3";

/********************************************
  
  Block.scss 
  
  All styles (except typography) for 
  drupal blocks. Start with abstractions
  and then inheritances underneath
  
********************************************/

//This is here only to define it.
.flex-video{ position: relative; }

/* Default Base Block Styles */
.block {
}

.block ul.menu,
.block ul.menu li {
  margin: 0;
  padding: 0;
}

.block > h2, .block > h3 {
  font-size: 1em;	
  margin:0px 0px 10px;
  font-weight: bold;
}
.block table { margin:0px; }

.block-search {
  width: 200px;
  float: right;
}

.block h3 {
  color: #2a2724;
  clear: both;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 1em; /* 15px relative to 15px */
  font-weight: bold;
  /* padding: 0 20px; */
}

.dc-sub > div {
  margin-top: 35px;
}


/* Admin menu block */
#block-menu-menu-authenticated {
  background: #fff;
  color:#8C8B89;
  @include border-radius(5px);
  -moz-box-shadow: 0 0 5px #888;
  -webkit-box-shadow: 0 0 5px#888;
  box-shadow: 0 0 5px #888;
  height:auto;
  padding:10px;
  position:absolute;
  right:-175px;
  top:35px;
  z-index:1000;
  
  ul {
    list-style: none;
  }
  
  h2 {
    background: url("../images/resources-gear.png") no-repeat scroll top left transparent;
    color:#8C8B89;
    cursor: pointer;
    height:36px;
    margin: 0;
    padding:5px 0 0 46px;
  }
  
  .block-content {
    padding:0 0 0 46px;
  }
}

/* Share this block */
#block-block-11 {
  position: absolute;
  right: 20px;
  top: -30px;
}

#block-block-11 .st_sharethis {
  background: url("../images/share-icon.png") no-repeat scroll left center transparent !important;
  height: 20px;
}

#block-block-11 .st_sharethis {
  padding: 1px 0 1px 22px;
  text-decoration: none;
}

#block-block-11 .stButton {
  margin: 0;
}

#block-block-11 .stButton .chicklets {
  background: none;
  padding: 0;
}

/* Do I have Access Block */


.front #block-menu-block-1 h2 {
  display: none;
}

.front #block-menu-block-1 ul.menu {
  list-style: none;
}

.front #block-menu-block-1 ul.menu li a {
  background: #18729d;
  color: #fff;
  @include border-radius(7px);
  display: block;
  font-family: "minion-pro-1","minion-pro-2", Georgia, 'Times New Roman', serif;
  font-size: 1.00em; /* 18px relative to 15px */
  font-weight: bold;
  height: 40px;
  line-height: 1;
  padding: 8px 20px 0;
  text-align: center;
  text-decoration: none;
  text-transform: uppercase;
}


/*  In this Section Block */


.not-front #block-menu-block-1 {
  background: none repeat scroll 0 0 #e0e0e0;
  border-bottom: 5px solid #c5c5c5;
  border-top: 5px solid #c5c5c5;
  font-family: "minion-pro-1","minion-pro-2", Georgia, 'Times New Roman', serif;
  padding: 20px;
}

.not-front #block-menu-block-1 ul {
  list-style:none;
}

.not-front #block-menu-block-1 li {
  padding: 4px 20px 4px;
  margin-left: -20px;
}

.not-front #block-menu-block-1 li.active,
.not-front #block-menu-block-1 li.active-trail {
  border-left: 5px solid #ababab;
}

.not-front #block-menu-block-1 li a {
  display: block;
  font-size: 1.067em; /* 16px relative to 15px */
  line-height: 1;
  text-decoration: none;
}

.not-front #block-menu-block-1 li a.active,
.not-front #block-menu-block-1 li a.active-trail {
  color: #104e6b;
  font-weight: bold;
}

/* Third and Fourth level */
.not-front #block-menu-block-1 li.active-trail li.active-trail,
.not-front #block-menu-block-1 li.active-trail li.active-trail li.active-trail {
  border:none;
}

/* Utilities Block */

#block-re-utilities-utility-nav {
  float:right;
}

#block-re-utilities-utility-nav .block-content {
  background: none;  
}

#block-re-utilities-utility-nav ul.links {
  height: 40px;
  padding: 0;
  list-style: none;
}

#block-re-utilities-utility-nav ul.links li {
  background: #f68115;
  border-bottom: 5px solid #ff932f;
  font-family: "minion-pro-1","minion-pro-2", Georgia, 'Times New Roman', serif;
  font-weight: bold;
  height: 43px;
  float: left;
  margin: 0;
  text-align: center;
  text-transform: uppercase;
  padding-right: 5px
}

#block-re-utilities-utility-nav ul.links li.first {
  border-left: 5px solid #ff932f;
  display: block;
}

#block-re-utilities-utility-nav ul.links li.last {
  border-right: 5px solid #ff932f;
  display: block;
  
  /*
    @media(max-width: $screenSmall){
    width: 25px;
    text-indent: -9999px;
  }*/
  
}

#block-re-utilities-utility-nav ul.links li a {
  color: #fff;
  display: block;
  padding-top: 13px;
  text-decoration: none;
  padding-left: 30px;
}

#block-re-utilities-utility-nav ul.links li a:hover {
  text-decoration: underline;
}

#block-re-utilities-utility-nav ul.links li a.register {
  
}

#block-re-utilities-utility-nav ul.links li a.dashboard {
  font-family: Helvetica, Arial, sans-serif;
  font-size: 0.786em; /* 11px relative to 14px (ul.links) */  
  padding-top: 25px;
}


.logged-in #block-re-utilities-utility-nav {
  /*  width: 380px; */
}

.logged-in #block-re-utilities-utility-nav .block-content .welcome {    
  font-family: Helvetica, Arial, sans-serif;
  position: absolute;
  right: 243px;
}

.logged-in #block-re-utilities-utility-nav ul.links li {
  border-left: 5px solid #ff932f;
}

#block-re-utilities-utility-nav ul.links li a.register {
  background: url('../images/icon-register.png') no-repeat 11px 12px;
}

#block-re-utilities-utility-nav ul.links li a.login, #block-re-utilities-utility-nav ul.links li a.logout {
  background: url('../images/icon-loginout.png') no-repeat 5px 12px;
}

#block-re-utilities-utility-nav ul.links li a.logout {
}

#block-re-utilities-utility-nav ul.links li a.search {
  background: url('../images/icon-search.png') no-repeat 5px 12px;
}



.logged-in #block-re-utilities-utility-nav ul.links li.first {
  background: none;
  border: none;
  font-family: Helvetica, Arial, sans-serif;
}

.logged-in #block-re-utilities-utility-nav ul.links li.last {
  border-left: none;
  border-right: 5px solid #ff932f;
}

.logged-in #block-re-utilities-utility-nav ul.links li.first {
  margin: 5px 5px 0 0;
  text-transform: none;
  width: 150px;
}

.logged-in #block-re-utilities-utility-nav ul.links li.first a {
  color: #18729d;
  display: block;
  float: right;
  font-weight: normal;
  padding-left: 0;
}


/* Footer Logo */

#block-block-1 {
  float: left;
  height: 75px;
  width: 255px;
}

/* Footer Text */

#block-block-10 {
  float:right;
}

#block-block-10 p {
  color: #807b77;
  margin-bottom: 0.5em;
  text-align: right;
}


/* Footer menu */

#block-menu-menu-footer {
  float: left;
}


#block-menu-menu-footer ul.menu li {
  float: left;
  list-style: none;
  list-style-image: none;
  padding-right: 0;
}

#block-menu-menu-footer ul.menu li a {
  border-right: 1px solid #807b77;
  color: #17729d;
  display: block;
  padding: 0 6px;
  text-decoration: none;
}

#block-menu-menu-footer ul.menu li.first a {
  padding-left: 0;
}

#block-menu-menu-footer ul.menu li.last a {
  border-right: none;
}


/*  Member/User Login Block */    
#block-user-login {
  display: none;
}

#block-user-login {
  background: #e0e0e0;
  border-top: 5px solid #d1d1d1;
  border-bottom: 5px solid #d1d1d1;
  padding: 0 15px;
  position: absolute;
  right: 35px;
  top: 40px;
  width: 204px;
  z-index: 100;
}

#block-user-login h2 {
  color: #807b77;
  font-family: "minion-pro-1","minion-pro-2", Georgia, 'Times New Roman', serif;
  margin-top: 20px;
  text-transform: uppercase;
}

#block-user-login label {
  font-family: Helvetica, Arial, sans-serif;
}

#block-user-login .form-text {
  width: 175px;
}

#block-user-login .form-submit,
#bd #block-user-0 .form-submit {
  background: #18729d;
  border: 0;
  @include border-radius(3px);
  color: #fff;
  font-family: "minion-pro-1","minion-pro-2", Georgia, 'Times New Roman', serif;
  font-size: 0.933em; /* 14px relative to 15px */
  height: 30px;
  text-transform: uppercase;
  width: 105px;
}


#block-user-login form .item-list ul {
  margin: 10px 0;
  padding: 0;
}

#block-user-login form .item-list ul li {
  list-style: none;
  list-style-image: none;
  margin: 0;
  padding: 0;
}

#block-user-login form .item-list ul li a {
  font-size: 0.867em; /* 13px relative to 15px */
  text-decoration: none;
}

#block-user-login .item-list li.first {
  display: none;
}

/* Committee/Group pages login form */
#bd #block-user-0 {
  float:left;
  width:215px;
}

#bd #block-user-0 .item-list {
  display: none;
}


/* What's New Sidebar Blocks and User Login block on Committee pages */

.dc-sub a {
  text-decoration: none;
}

#block-views-blog-block-2,  
#block-views-blog-block-3, 
#block-views-events-block-1,
#block-views-events-block-2,
#block-views-trials-block-1,
#block-user-0 {
  background: none repeat scroll 0 0 #efefef;
  border-bottom: 5px solid #e0e0e0;
  border-top: 5px solid #e0e0e0;
  font-family: "minion-pro-1","minion-pro-2", Georgia, 'Times New Roman', serif;
  padding: 20px;
}


/* Organizations */

#block-views-resources-block_1 h3,
#block-views-resources-block_4 h3 {
  color: #F68115;
  font-size: 1.133em; /* 17px relative to 15px */
  font-weight: bold;
}

#block-views-contacts-block_1 {
  margin:0 0 10px 0;
}

#block-views-contacts-block_1 .view-contacts {
  margin-top:0;
}



